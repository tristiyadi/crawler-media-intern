{
    "media_internasional":{
       "mappings":{
          "article":{
             "dynamic_templates":[
                {
                   "notanalyzed":{
                      "mapping":{
                         "index":"not_analyzed",
                         "type":"string"
                      },
                      "match":"*",
                      "match_mapping_type":"string"
                   }
                }
             ],
             "properties":{
                "sentimen":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "url":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "link":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "title":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "description":{
                    "type":"string",
                    "index":"not_analyzed"
                },
                "timestamp": {
                    "type":"string",
                    "index":"not_analyzed"
                },
                "sourceId": {
                    "type":"string",
                    "index":"not_analyzed"
                },
                "name_news": {
                    "type":"string",
                    "index":"not_analyzed"
                },
                "base_url": {
                    "type":"string",
                    "index":"not_analyzed"
                },
                "updated_by":{
                   "type":"string",
                   "index":"not_analyzed"
                },
                "updated_date":{
                   "type":"date",
                   "format":"yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||epoch_millis"
                }
             }
          }
       }
    }
 }
